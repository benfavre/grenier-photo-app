{
  "$schema": "https://json.schemastore.org/package",
  "name": "elysia-kickstart",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "----RUN": "",
    "start": "bun run tw && bun run src/index.tsx",
    "dev": "concurrently \"bun run --watch src/index.tsx\" \"bun run tw --watch\"",
    "tw": "tailwindcss -i ./src/globals.css -o ./dist/globals.css",
    "----CHECK": "",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:fix": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix . ",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/html": "^0.6.5",
    "@elysiajs/static": "^0.6.0",
    "elysia": "^0.6.23",
    "typed-html": "^3.0.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2"
  }
}
